{
  "name": "jcase",
  "version": "1.0.0",
  "description": "whitefur jcase",
  "scripts": {
    "dev": "set NODE_ENV=dev && gulp",
    "publish": "set NODE_ENV=publish && gulp",
    "build": "set NODE_ENV=build && gulp",
    "dev:osx": "NODE_ENV=dev gulp",
    "publish:osx": "NODE_ENV=publish gulp",
    "build:osx": "NODE_ENV=build gulp"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "brfs"
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "react": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "standard": {
    "globals": [
      "$",
      "Image"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browser-sync": "^2.18.8",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "connect-route": "^0.1.5",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-changed": "^2.0.0",
    "gulp-clean-css": "^3.0.4",
    "gulp-derequire": "^2.1.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-if": "^2.0.2",
    "gulp-notify": "^3.0.0",
    "gulp-pug": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-standard": "^10.0.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "lodash.assign": "^4.2.0",
    "pretty-hrtime": "^1.0.3",
    "require-dir": "^0.3.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/whitefurwang/jcase"
  }
}
